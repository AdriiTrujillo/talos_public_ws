<?xml version="1.0" encoding="UTF-8"?>

<launch>
      
    <!-- Load controllers -->
    <rosparam command="load" file="$(find astronaut_controllers)/config/right_arm_position.yaml" />
    
    <!-- Controllers -->
    <node name="controller_spawner" pkg="controller_manager" type="spawner"
        respawn="false" output="screen"
        args="
        joint_1_astro_right_arm
        joint_2_astro_right_arm
        joint_3_astro_right_arm
        joint_4_astro_right_arm
        joint_5_astro_right_arm
        joint_6_astro_right_arm
        joint_7_astro_right_arm
        --timeout 60">
    </node>
    
    <!-- rqt -->
    <node name="rqt_reconfigure" pkg="rqt_reconfigure" type="rqt_reconfigure" />
    <node name="rqt_publisher" pkg="rqt_publisher" type="rqt_publisher" />

          
</launch>
